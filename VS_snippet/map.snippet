<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <!-- 标题 -->
      <Title>map创建</Title>
      <!-- 缩写代码 -->
      <Shortcut>map</Shortcut>   
      <!-- 代码描述 -->
      <Description>无</Description>
      <!-- 作者 -->
      <Author>Fau</Author>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>key</ID>
          <ToolTip>key值</ToolTip>
          <Default>int</Default>
        </Literal>
        <Literal>
          <ID>value</ID>
          <ToolTip>value值</ToolTip>
          <Default>int</Default>
        </Literal>
        <Literal>
          <ID>name</ID>
          <ToolTip>容器名称</ToolTip>
          <Default>mp</Default>
        </Literal>
      </Declarations>
      <Code Language="cpp">
        <![CDATA[map<$key$,$value$> $name$;$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>