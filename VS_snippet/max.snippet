<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <!-- 标题 -->
      <Title>max()</Title>
      <!-- 缩写代码 -->
      <Shortcut>max</Shortcut>   
      <!-- 代码描述 -->
      <Description>两数取大</Description>
      <!-- 作者 -->
      <Author>Fau</Author>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>text1</ID>
          <ToolTip>变量1</ToolTip>
          <Default>a</Default>
        </Literal>
        <Literal>
          <ID>text2</ID>
          <ToolTip>变量2</ToolTip>
          <Default>b</Default>
        </Literal>
      </Declarations>
      <Code Language="cpp">
        <![CDATA[max($text1$, $text2$);$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>